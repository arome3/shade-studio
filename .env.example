# NEAR Network Configuration
# Options: mainnet, testnet
NEXT_PUBLIC_NEAR_NETWORK=testnet
NEXT_PUBLIC_NEAR_CONTRACT_ID=private-grant-studio.testnet

# NEAR Social / BOS Configuration
NEXT_PUBLIC_SOCIAL_CONTRACT_ID=v1.social08.testnet

# AI Service Configuration (Phala TEE)
# Get your API key from https://ai.phala.network
NEXT_PUBLIC_AI_ENDPOINT=https://ai.phala.network/v1
AI_API_KEY=

# IPFS Storage (Pinata)
# Get your API keys from https://app.pinata.cloud/developers/api-keys
PINATA_API_KEY=
PINATA_SECRET_KEY=
NEXT_PUBLIC_IPFS_GATEWAY=https://gateway.pinata.cloud/ipfs

# Encryption Configuration
NEXT_PUBLIC_ENCRYPTION_VERSION=1

# Feature Flags
NEXT_PUBLIC_ENABLE_ZK_PROOFS=true
NEXT_PUBLIC_ENABLE_AI_FEATURES=true
NEXT_PUBLIC_ENABLE_DAILY_BRIEFINGS=true

# Chain Signatures (Cross-Chain Submission)
# Enable cross-chain grant submission via NEAR MPC signatures
NEXT_PUBLIC_ENABLE_CHAIN_SIGNATURES=false
# Optional: Override default public RPC endpoints
# NEXT_PUBLIC_ETH_RPC_URL=https://eth.llamarpc.com
# NEXT_PUBLIC_OP_RPC_URL=https://mainnet.optimism.io
# NEXT_PUBLIC_ARB_RPC_URL=https://arb1.arbitrum.io/rpc
# NEXT_PUBLIC_POLYGON_RPC_URL=https://polygon-rpc.com
# NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org

# Async AI Pipelines
NEXT_PUBLIC_ASYNC_AI_CONTRACT_ID=async-ai.testnet
NEXT_PUBLIC_ENABLE_ASYNC_AI=true
# Worker daemon credentials (for scripts/async-ai-worker.ts only)
# ASYNC_AI_WORKER_ACCOUNT_ID=worker.testnet
# ASYNC_AI_WORKER_PRIVATE_KEY=ed25519:...

# Development
NEXT_PUBLIC_DEBUG_MODE=false
